// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model bookdata {
  asin       String
  author     String
  image_url  String
  rating     String
  title      String @unique
  url        String
  categories String
  ISBN10     String @id
}

model User {
  id                 String             @id @default(uuid())
  firstName          String
  username           String?            @unique
  email              String             @unique
  googleId           String             @unique
  lastName           String?
  picture            String?
  password           String?
  latitude           Float?
  longitude          Float?
  radius             Int?
  NotificationsCount Int                @default(0)
  phoneNumber        String?

}